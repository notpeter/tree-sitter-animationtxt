{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "animationtxt",
  "rules": {
    "document": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "(\\r?\\n)*"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "statement"
              },
              {
                "type": "PATTERN",
                "value": "(\\r?\\n)+"
              },
              {
                "type": "STRING",
                "value": "\u0000"
              }
            ]
          }
        }
      ]
    },
    "statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "loopCount"
        },
        {
          "type": "SYMBOL",
          "name": "frames"
        },
        {
          "type": "SYMBOL",
          "name": "introFrames"
        }
      ]
    },
    "loopCount": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "loopCount"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": " +"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": " +"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "integer"
        }
      ]
    },
    "frames": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "frames"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": " +"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": " +"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "framelist"
        }
      ]
    },
    "introFrames": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "introFrames"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": " +"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": " +"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "framelist"
        }
      ]
    },
    "framelist": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "integer"
            },
            {
              "type": "SYMBOL",
              "name": "frameRepeat"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "PATTERN",
                "value": " *, *"
              },
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "integer"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "frameRepeat"
                  }
                ]
              }
            ]
          }
        }
      ]
    },
    "frameRepeat": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "PATTERN",
            "value": "[1-9][0-9]*"
          },
          {
            "type": "IMMEDIATE_TOKEN",
            "content": {
              "type": "STRING",
              "value": "x"
            }
          },
          {
            "type": "IMMEDIATE_TOKEN",
            "content": {
              "type": "PATTERN",
              "value": "[1-9][0-9]*"
            }
          }
        ]
      }
    },
    "integer": {
      "type": "PATTERN",
      "value": "[1-9][0-9]*"
    }
  },
  "extras": [],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}